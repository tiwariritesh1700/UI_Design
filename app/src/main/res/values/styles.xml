<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="InfoTheme" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="colorAccent">@color/info</item>
    </style>

    <style name="SucessTheme" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="colorAccent">@color/sucess</item>
    </style>

    <style name="WarningTheme" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="colorAccent">@color/warning</item>
    </style>

    <style name="AlertTheme" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="colorAccent">@color/alert</item>
    </style>


</resources>
